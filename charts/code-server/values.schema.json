{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "certificate": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "dnsNames": {
                    "type": "array"
                },
                "issuerRef": {
                    "type": "object",
                    "properties": {
                        "kind": {
                            "type": "string"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "labels": {
                    "type": "object"
                },
                "secretName": {
                    "type": "string"
                }
            }
        },
        "configMap": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "config": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "existingConfigMap": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "mountPath": {
                    "type": "string"
                }
            }
        },
        "controller": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "kind": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "replicas": {
                    "type": "integer"
                }
            }
        },
        "cronJob": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "failedJobsHistoryLimit": {
                    "type": "integer"
                },
                "labels": {
                    "type": "object"
                },
                "schedule": {
                    "type": "string"
                },
                "successfulJobsHistoryLimit": {
                    "type": "integer"
                },
                "suspend": {
                    "type": "boolean"
                }
            }
        },
        "dnsConfig": {
            "type": "object"
        },
        "env": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                }
            }
        },
        "extraArgs": {
            "type": "array"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingressRoute": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "entryPoints": {
                    "type": "array"
                },
                "labels": {
                    "type": "object"
                },
                "middlewares": {
                    "type": "array"
                },
                "rule": {
                    "type": "string"
                },
                "tlsSecretName": {
                    "type": "string"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "persistentVolumeClaim": {
            "type": "object",
            "properties": {
                "accessMode": {
                    "type": "string"
                },
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "ensurePermissions": {
                    "type": "boolean"
                },
                "existingPersistentVolumeClaim": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "mountPath": {
                    "type": "string"
                },
                "projectsPath": {
                    "type": "string"
                },
                "size": {
                    "type": "string"
                },
                "storageClassName": {
                    "type": "string"
                },
                "volumeMode": {
                    "type": "string"
                }
            }
        },
        "ports": {
            "type": "object",
            "properties": {
                "http": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "nodePort": {
                            "type": "null"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "protocol": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "repositories": {
            "type": "array"
        },
        "resources": {
            "type": "object"
        },
        "secret": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "existingSecret": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "fsGroup": {
                    "type": "integer"
                },
                "runAsGroup": {
                    "type": "integer"
                },
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "runAsUser": {
                    "type": "integer"
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "labels": {
                    "type": "object"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
