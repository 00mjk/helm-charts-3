{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "certificate": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "dnsNames": {
                    "type": "array"
                },
                "issuerRef": {
                    "type": "object",
                    "properties": {
                        "kind": {
                            "type": "string"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "labels": {
                    "type": "object"
                },
                "secretName": {
                    "type": "string"
                }
            }
        },
        "configMap": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "existingConfigMap": {
                    "type": "string"
                },
                "files": {
                    "type": "object"
                },
                "labels": {
                    "type": "object"
                },
                "mountPath": {
                    "type": "string"
                }
            }
        },
        "configMapReload": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "extraArgs": {
                    "type": "array"
                },
                "extraConfigMapMounts": {
                    "type": "array"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "resources": {
                    "type": "object"
                }
            }
        },
        "deployment": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "kind": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "replicas": {
                    "type": "integer"
                },
                "strategy": {
                    "type": "object",
                    "properties": {
                        "rollingUpdate": {
                            "type": "object",
                            "properties": {
                                "maxSurge": {
                                    "type": "integer"
                                },
                                "maxUnavailable": {
                                    "type": "integer"
                                }
                            }
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "env": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                }
            }
        },
        "extraArgs": {
            "type": "array"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingressRoute": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "entryPoints": {
                    "type": "array"
                },
                "labels": {
                    "type": "object"
                },
                "middlewares": {
                    "type": "array"
                },
                "rule": {
                    "type": "string"
                },
                "tlsSecretName": {
                    "type": "string"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "persistentVolumeClaim": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "existingPersistentVolumeClaim": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "mountPath": {
                    "type": "string"
                }
            }
        },
        "ports": {
            "type": "object",
            "properties": {
                "http": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "port": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "rbac": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
