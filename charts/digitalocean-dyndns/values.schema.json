{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "cronJob": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "failedJobsHistoryLimit": {
                    "type": "integer"
                },
                "labels": {
                    "type": "object"
                },
                "nameservers": {
                    "type": "array"
                },
                "schedule": {
                    "type": "string"
                },
                "successfulJobsHistoryLimit": {
                    "type": "integer"
                },
                "suspend": {
                    "type": "boolean"
                }
            }
        },
        "env": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "value": {
                        "type": "string"
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "resources": {
            "type": "object"
        },
        "secret": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "existingSecret": {
                    "type": "string"
                },
                "labels": {
                    "type": "object"
                },
                "token": {
                    "type": "string"
                }
            }
        }
    }
}
